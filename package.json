{
  "name": "@nikolaymatrosov/n8n-nodes-yandex360",
  "version": "0.0.1",
  "description": "n8n nodes for Yandex 360 services",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Nikolay Matrosov",
    "email": "nikolay.matrosov@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikolaymatrosov/n8n-nodes-yandex360.git"
  },
  "engines": {
    "node": ">=22.15.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && tsc-alias && gulp build:icons && npm run validate:nodes",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate:nodes": "node scripts/validate-nodes.js",
    "prepublishOnly": "npm run build && eslint -c eslint.config.prepublish.js --ignore-pattern '**/*.test.ts' nodes credentials package.json",
    "git-tag": "git tag v`cat package.json | jq -r '.version' | awk -F. '{print $1}'` -f &&  git tag v`cat package.json | jq -r '.version'` -f",
    "changelog:promote": "node scripts/promote-changelog.js"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Yandex360OAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Yandex360Disk/Yandex360Disk.node.js",
      "dist/nodes/Yandex360Disk/Yandex360DiskTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.1",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "globals": "^16.5.0",
    "gulp": "^5.0.1",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "n8n-core": "^1.117.0",
    "nock": "^14.0.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "generate-schema": "^2.6.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.6.0",
    "yd-sdk": "^1.2.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
